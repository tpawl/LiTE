        ":package_name"
    ],
    "homepage": "https://github.com/tpawl/LiTE",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Pawlitschko",
            "homepage": "https://github.com/tpawl",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3"
    },
    "autoload": {
        "psr-4": {
